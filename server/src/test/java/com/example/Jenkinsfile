pipeline {
    agent any {
        stages {
            stage ('init') {
                steps {
                    echo "This is Initializing Stage"
                }
            }
            stage ('build') {
                steps {
                    echo "This is Build Satge"
                }
            }
            stage ('Deploy') {
                steps {
                    echo "This is Deployment Satge"
                }
            }
        }
    }
}